{
  "name": "rhombus-test-project",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "setup": "npm install @playwright/test && npm install && npx playwright install chromium",
    "test:ui": "npx playwright test ui-tests/transformations.spec.js",
    "test:ui-headed": "npx playwright test ui-tests/transformations.spec.js --headed",
    "test:ui-debug": "npx playwright test ui-tests/transformations.spec.js --debug",
    "test:get-session-token": "npx playwright test ui-tests/get-login-session-token.spec.js",
    "test:api": "npx playwright test api-tests/",
    "data-validate": "node data-validation/validate-transformations.js",
    "report": "npx playwright show-report"
  },
  "keywords": [],
  "author": "Kaan Duran",
  "license": "ISC",
  "description": "A collection of Playwright browser and API tests for Rhombus AI's web application.",
  "devDependencies": {
    "dotenv": "^17.2.3",
    "playwright": "^1.57.0"
  },
  "dependencies": {
    "@playwright/test": "^1.57.0"
  }
}
